			<form action="${pageContext.request.contextPath}/spinfo/addgys.do"
				method="get">
				<!-- 正文区域 -->
				<div class="panel panel-default">
					<div class="panel-heading">供应商信息</div>
					<div class="row data-type">

						<div class="col-md-2 title">供应商名称</div>
						<div class="col-md-4 data">
							<input type="text" class="form-control" name="name"
								placeholder="供应商名称" value="">
						</div>
						<div class="box-tools text-left">
							<button type="submit" class="btn bg-maroon" >保存</button>
						</div>
					</div>
<%--				<div class="box-tools text-center">--%>
<%--					<button type="submit" class="btn bg-maroon" >保存</button>--%>
<%--				</div>--%>
				<!-- 正文区域 /-->
			</form>
			<table id="dataList"
				   class="table table-bordered table-striped table-hover dataTable">
				<thead>
				<tr>

					<th class="sorting_asc">序号</th>
					<th class="sorting_asc">数据库ID</th>
					<th class="sorting_desc">供应商名称</th>
					<th class="sorting_desc">操作</th>
				</tr>
				</thead>
				<tbody>

				<c:forEach items="${gysList}" var="gys" varStatus="var">
					<tr>
						<td>${var.count}</td>
						<td>${gys.id}</td>
						<td>${gys.name }</td>
						<td class="text-center">
							<li class="subTypeLi"><a onclick="onckone('${gys.id }')">删除</a></li>
						</td>
					</tr>
				</c:forEach>
